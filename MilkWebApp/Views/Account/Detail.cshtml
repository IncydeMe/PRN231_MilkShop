@using MilkData.Models
@{
    ViewData["Title"] = "Detail";
}

@model Account

<div class="form-group row">
    <div class="col-md-6">
        <label for="1">Type</label>
        @Html.HiddenFor(model => Model.AccountId)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-6">
        <label for="1">FullName:</label>
        @Html.EditorFor(model => Model.FullName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Email:</label>
        @Html.EditorFor(model => Model.Email, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Password:</label>
        @Html.EditorFor(model => Model.Password, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Phone:</label>
        @Html.EditorFor(model => Model.Phone, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Address:</label>
        @Html.EditorFor(model => Model.Address, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Role:</label>
        @Html.EditorFor(model => Model.Role, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Point:</label>
        @Html.EditorFor(model => Model.Point, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">IsActive</label>
        @Html.CheckBoxFor(model => Model.IsActive, new { htmlAttributes = new { @class = "form-check-input" } })
    </div>
</div>