@using MilkData.Models
@{
    ViewData["Title"] = "Add";
}

@model BlogCategory

<div class="form-group row">
    <div class="col-md-6">
        <label for="1">BlogCategoryId</label>
        @Html.HiddenFor(model => model.BlogCategoryId)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-6">
        <label for="1">Blog Category Name:</label>
        @Html.EditorFor(model => model.BlogCategoryName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
</div>
